{
  "name": "switcheroony",
  "displayName": "Switcheroony",
  "publisher": "yairhaimo",
  "description": "Easily switch between files",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.43.0"
  },
  "repository": {
    "url": "https://github.com/yairhaimo/switcheroony"
  },
  "icon": "images/logo.png",
  "galleryBanner": {
    "color": "#ff7d57",
    "theme": "dark"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:javascript",
    "onLanguage:javascriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.switcheroonyToTest",
        "title": "Switch to Test File"
      },
      {
        "command": "extension.switcheroonyToImplementation",
        "title": "Switch to Production Code"
      },
      {
        "command": "extension.switcheroonyToDriver",
        "title": "Switch to Driver"
      },
      {
        "command": "extension.switcheroonyToStyle",
        "title": "Switch to Style"
      }
    ],
    "keybindings": [
      {
        "command": "extension.switcheroonyToTest",
        "key": "ctrl+alt+down",
        "mac": "ctrl+alt+down"
      },
      {
        "command": "extension.switcheroonyToImplementation",
        "key": "ctrl+alt+up",
        "mac": "ctrl+alt+up"
      },
      {
        "command": "extension.switcheroonyToDriver",
        "key": "ctrl+alt+right",
        "mac": "ctrl+alt+right"
      },
      {
        "command": "extension.switcheroonyToStyle",
        "key": "ctrl+alt+left",
        "mac": "ctrl+alt+left"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/lodash.memoize": "^4.1.6",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.43.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "lodash.memoize": "^4.1.2"
  }
}
